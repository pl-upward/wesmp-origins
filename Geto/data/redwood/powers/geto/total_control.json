{
    "type": "origins:multiple",
    "aggro_mobs_attack": {
        "type": "origins:target_action_on_hit",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "execute as @e[team=geto.controlled] run damage @s 0.0 minecraft:bad_respawn_point by @e[limit=1,sort=nearest,distance=..0.1]"
        },
        "cooldown": 1
    },
    "aggro_mobs_attacked": {
        "type": "origins:attacker_action_when_hit",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "execute as @e[team=geto.controlled] run damage @s 0.0 minecraft:bad_respawn_point by @e[limit=1,sort=nearest,distance=..0.1]"
        },
        "cooldown": 1
    },
    "commands": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players set @s list_mobs 0"
                },
                {
                    "type": "origins:execute_command",
                    "command": "function redwood:geto/list_mobs"
                }
            ]
        },
        "interval": 1,
        "condition": {
            "type": "origins:scoreboard",
            "objective": "list_mobs",
            "comparison": ">",
            "compare_to": 0
        }
    },
    "command_permission": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:execute_command",
            "command": "scoreboard players enable @s list_mobs"
        }      
    },
    "name": "Complete Control",
    "description": "Mobs that you resummon will fight for you."
}